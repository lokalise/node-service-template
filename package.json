{
  "name": "node-service-template",
  "version": "1.0.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "copy:config": "cpy --rename=.env ./.env.default ./",
    "test": "cross-env DATABASE_URL=mysql://root:rootpass@localhost:3306/service_db_test vitest",
    "test:migrate": "cross-env DATABASE_URL=mysql://root:rootpass@localhost:3306/service_db_test npx prisma migrate reset --force",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm run copy:config && npm run test:migrate && npm run test:coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint .  --ext .ts --fix",
    "format": "prettier --write . && npx prisma format",
    "start:dev": "cross-env NODE_ENV=development ts-node -r dotenv/config src/server.ts",
    "start:dev:watch": "cross-env NODE_ENV=development ts-node-dev --clear --exit-child --watch src -r dotenv/config src/server.ts",
    "start:prod": "tsc && cross-env NODE_ENV=production node -r dotenv/config dist/src/server.js",
    "docker:start:dev": "docker compose up -d",
    "docker:stop:dev": "docker compose down",
    "db:start:dev": "docker compose up -d mysql redis",
    "db:stop:dev": "docker compose down",
    "db:migration:dev": "npx prisma migrate dev",
    "db:migration:run": "npx prisma migrate deploy",
    "db:update-client": "npx prisma generate",
    "spec:generate": "cross-env NODE_ENV=development ts-node -r dotenv/config scripts/generateOpenApi.ts && npm run spec:validate",
    "spec:validate": "cross-env NODE_ENV=development ts-node -r dotenv/config scripts/validateOpenApi.ts",
    "prepublishOnly": "npm run build",
    "jwt:generate": "cross-env NODE_ENV=development ts-node scripts/generateJwt.ts"
  },
  "dependencies": {
    "@fastify/auth": "^4.3.0",
    "@fastify/awilix": "^3.1.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^10.1.1",
    "@fastify/jwt": "^7.1.0",
    "@fastify/schedule": "^4.1.1",
    "@fastify/swagger": "^8.6.0",
    "@fastify/swagger-ui": "^1.9.0",
    "@lokalise/fastify-extras": "^9.0.0",
    "@lokalise/node-core": "^5.8.1",
    "@lokalise/zod-extras": "^2.0.0",
    "@message-queue-toolkit/amqp": "^1.0.3",
    "@message-queue-toolkit/core": "^1.0.0",
    "@prisma/client": "^4.16.1",
    "@prisma/instrumentation": "^4.16.1",
    "amqplib": "^0.10.3",
    "awilix": "^8.0.1",
    "awilix-manager": "^1.2.0",
    "croner": "^6.0.5",
    "dotenv": "^16.3.1",
    "fastify": "^4.18.0",
    "fastify-custom-healthcheck": "^3.1.0",
    "fastify-graceful-shutdown": "^3.4.0",
    "fastify-metrics": "^10.3.0",
    "fastify-no-icon": "^6.0.0",
    "fastify-type-provider-zod": "^1.1.9",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "layered-loader": "^11.0.1",
    "newrelic": "10.3.1",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "redis-semaphore": "^5.3.1",
    "toad-scheduler": "^3.0.0",
    "undici": "^5.22.1",
    "validation-utils": "^7.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/newrelic": "^9.14.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/coverage-v8": "^0.32.2",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.2.6",
    "mockttp": "^3.7.5",
    "oas-validator": "^5.0.8",
    "prettier": "^2.8.8",
    "prisma": "^4.16.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.1.3",
    "vitest": "^0.32.2",
    "yaml": "^2.3.1"
  }
}
