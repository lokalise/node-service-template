{
  "name": "node-service-template",
  "version": "1.0.0",
  "engines": {
    "node": ">=22.10.0"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "copy:config": "cpy --rename=.env ./.env.default ./",
    "test": "vitest",
    "test:migrate": "cross-env DATABASE_URL=postgres://serviceuser:pass@localhost:5432/service_db_test npx prisma migrate reset --force",
    "test:coverage": "node --run test -- --coverage",
    "test:ci": "node --run copy:config && node --run test:migrate && node --run test:coverage",
    "lint": "biome check . && npx prisma-lint -c ./.prismalintrc.json && tsc --project tsconfig.lint.json --noEmit",
    "lint:fix": "biome check --write . && npx prisma format",
    "start:dev": "cross-env NODE_ENV=development tsx --env-file=.env src/server.ts",
    "start:dev:watch": "cross-env NODE_ENV=development tsx watch --env-file=.env src/server.ts",
    "start:prod": "tsc && cross-env NODE_ENV=production node --env-file-if-exists=.env dist/src/server.js",
    "free-ports": "npx cross-port-killer 3000",
    "docker:start:dev": "docker compose up -d",
    "docker:stop:dev": "docker compose down",
    "db:start:dev": "docker compose up -d postgres redis",
    "db:stop:dev": "docker compose down",
    "db:migration:dev": "npx prisma migrate dev",
    "db:migration:run": "npx prisma migrate deploy",
    "db:update-client": "npx prisma generate",
    "db:wait": "while ! echo \"SELECT 1;\" | prisma db execute --stdin; do sleep 1; done",
    "spec:generate": "cross-env NODE_ENV=development tsx --env-file=.env scripts/generateOpenApi.ts && node --run spec:validate",
    "spec:validate": "cross-env NODE_ENV=development tsx --env-file=.env scripts/validateOpenApi.ts",
    "prepublishOnly": "node --run build",
    "jwt:generate": "cross-env NODE_ENV=development tsx scripts/generateJwt.ts",
    "cmd:getUserImportJobs:dev": "cross-env NODE_ENV=development tsx --env-file=.env scripts/cmd/getUserImportJobs.ts",
    "cmd:getUserImportJobs:prod": "node --env-file-if-exists=.env scripts/cmd/getUserImportJobs.js"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.687.0",
    "@aws-sdk/client-sqs": "^3.687.0",
    "@aws-sdk/credential-providers": "^3.687.0",
    "@fastify/auth": "^5.0.1",
    "@fastify/awilix": "^6.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/schedule": "^5.0.2",
    "@fastify/swagger": "^9.2.0",
    "@lokalise/backend-http-client": "^2.4.0",
    "@lokalise/background-jobs-common": "^8.0.0",
    "@lokalise/fastify-extras": "^25.2.0",
    "@lokalise/healthcheck-utils": "^1.4.0",
    "@lokalise/id-utils": "^2.2.0",
    "@lokalise/node-core": "^13.1.0",
    "@lokalise/zod-extras": "^2.1.0",
    "@message-queue-toolkit/amqp": "^16.2.0",
    "@message-queue-toolkit/core": "^17.2.1",
    "@message-queue-toolkit/schemas": "^4.0.0",
    "@message-queue-toolkit/sns": "^18.0.1",
    "@message-queue-toolkit/sqs": "^17.2.0",
    "@prisma/client": "^5.22.0",
    "@prisma/instrumentation": "^5.22.0",
    "@scalar/fastify-api-reference": "^1.25.61",
    "@supercharge/promise-pool": "^3.2.0",
    "amqplib": "^0.10.4",
    "awilix": "^12.0.3",
    "awilix-manager": "^5.5.0",
    "bullmq": "^5.25.4",
    "fastify": "^5.1.0",
    "fastify-custom-healthcheck": "^3.1.0",
    "fastify-graceful-shutdown": "^4.0.1",
    "fastify-metrics": "^12.1.0",
    "fastify-no-icon": "^7.0.0",
    "fastify-type-provider-zod": "^4.0.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "layered-loader": "^13.3.0",
    "newrelic": "12.6.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3",
    "redis-semaphore": "^5.6.1",
    "toad-scheduler": "^3.0.1",
    "undici": "^6.20.1",
    "validation-utils": "11.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@amplitude/analytics-types": "^2.8.4",
    "@biomejs/biome": "1.9.4",
    "@types/amqplib": "^0.10.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/newrelic": "^9.14.5",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^2.1.5",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "mockttp": "^3.15.4",
    "oas-validator": "^5.0.8",
    "prisma": "^5.22.0",
    "prisma-lint": "^0.7.0",
    "ts-deepmerge": "^7.0.1",
    "tsx": "^4.19.2",
    "type-fest": "^4.26.1",
    "typescript": "5.6.3",
    "vitest": "^2.1.3",
    "yaml": "^2.6.0"
  }
}
