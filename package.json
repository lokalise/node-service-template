{
  "name": "node-service-template",
  "version": "1.0.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "copy:config": "cpy --rename=.env ./.env.default ./",
    "test": "cross-env DATABASE_URL=mysql://root:rootpass@localhost:3306/service_db_test vitest",
    "test:migrate": "cross-env DATABASE_URL=mysql://root:rootpass@localhost:3306/service_db_test npx prisma migrate reset --force",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm run copy:config && npm run test:migrate && npm run test:coverage",
    "lint": "eslint . --ext .ts && npx prisma-lint -c ./.prismalintrc.json && prettier --check --log-level warn \"**/*.{json,md,ts}\"",
    "lint:fix": "eslint . --ext .ts --fix && prettier --write --log-level silent \"**/*.{json,md,ts}\" && npx prisma format",
    "start:dev": "cross-env NODE_ENV=development ts-node -r dotenv/config src/server.ts",
    "start:dev:watch": "cross-env NODE_ENV=development ts-node-dev --clear --exit-child --watch src -r dotenv/config src/server.ts",
    "start:prod": "tsc && cross-env NODE_ENV=production node -r dotenv/config dist/src/server.js",
    "free-ports": "npx cross-port-killer 3000",
    "docker:start:dev": "docker compose up -d",
    "docker:stop:dev": "docker compose down",
    "db:start:dev": "docker compose up -d mysql redis",
    "db:stop:dev": "docker compose down",
    "db:migration:dev": "npx prisma migrate dev",
    "db:migration:run": "npx prisma migrate deploy",
    "db:update-client": "npx prisma generate",
    "spec:generate": "cross-env NODE_ENV=development ts-node -r dotenv/config scripts/generateOpenApi.ts && npm run spec:validate",
    "spec:validate": "cross-env NODE_ENV=development ts-node -r dotenv/config scripts/validateOpenApi.ts",
    "prepublishOnly": "npm run build",
    "jwt:generate": "cross-env NODE_ENV=development ts-node scripts/generateJwt.ts"
  },
  "dependencies": {
    "@fastify/auth": "^4.6.1",
    "@fastify/awilix": "^5.1.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/schedule": "^4.1.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@lokalise/fastify-extras": "^16.6.0",
    "@lokalise/node-core": "^9.10.1",
    "@lokalise/zod-extras": "^2.0.1",
    "@message-queue-toolkit/amqp": "^10.0.0",
    "@message-queue-toolkit/core": "^8.0.1",
    "@prisma/client": "^5.10.2",
    "@prisma/instrumentation": "^5.10.2",
    "amqplib": "^0.10.3",
    "awilix": "^10.0.1",
    "awilix-manager": "^5.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-custom-healthcheck": "^3.1.0",
    "fastify-graceful-shutdown": "^3.5.1",
    "fastify-metrics": "^11.0.0",
    "fastify-no-icon": "^6.0.0",
    "fastify-type-provider-zod": "^1.1.9",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "layered-loader": "^13.0.2",
    "newrelic": "11.12.0",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "redis-semaphore": "^5.5.1",
    "toad-scheduler": "^3.0.1",
    "undici": "^6.7.1",
    "validation-utils": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@amplitude/analytics-types": "^2.5.0",
    "@types/amqplib": "^0.10.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/newrelic": "^9.14.3",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitest/coverage-v8": "^1.3.1",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vitest": "^0.3.26",
    "mockttp": "^3.10.1",
    "oas-validator": "^5.0.8",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "prisma-lint": "^0.1.0",
    "ts-deepmerge": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.4.2",
    "vitest": "^1.3.1",
    "yaml": "^2.4.1"
  }
}
