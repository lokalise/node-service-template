{
  "name": "node-service-template",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.12.2"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "copy:config": "cpy --rename=.env ./.env.default ./",
    "test": "vitest",
    "test:migrate": "cross-env DATABASE_URL=postgres://serviceuser:pass@localhost:5432/service_db_test npx prisma migrate reset --force",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm run copy:config && npm run test:migrate && npm run test:coverage",
    "lint": "biome check . && npx prisma-lint -c ./.prismalintrc.json && tsc --project tsconfig.lint.json --noEmit",
    "lint:fix": "biome check --write . && npx prisma format",
    "lint:fix-unsafe": "biome check --write --unsafe . && npx prisma format",
    "start:dev": "cross-env NODE_ENV=development tsx --env-file=.env src/server.ts",
    "start:dev:watch": "cross-env NODE_ENV=development tsx watch --env-file=.env src/server.ts",
    "start:prod": "tsc && cross-env NODE_ENV=production node --env-file=.env dist/src/server.js",
    "free-ports": "npx cross-port-killer 3000",
    "docker:start:dev": "docker compose up -d",
    "docker:stop:dev": "docker compose down",
    "db:start:dev": "docker compose up -d postgres redis",
    "db:stop:dev": "docker compose down",
    "db:migration:dev": "npx prisma migrate dev",
    "db:migration:run": "npx prisma migrate deploy",
    "db:update-client": "npx prisma generate",
    "db:wait": "while ! echo \"SELECT 1;\" | prisma db execute --stdin; do sleep 1; done",
    "spec:generate": "cross-env NODE_ENV=development tsx --env-file=.env scripts/generateOpenApi.ts && npm run spec:validate",
    "spec:validate": "cross-env NODE_ENV=development tsx --env-file=.env scripts/validateOpenApi.ts",
    "prepublishOnly": "npm run build",
    "jwt:generate": "cross-env NODE_ENV=development tsx scripts/generateJwt.ts",
    "cmd:getUserImportJobs:dev": "cross-env NODE_ENV=development tsx --env-file=.env scripts/cmd/getUserImportJobs.ts",
    "cmd:getUserImportJobs:prod": "node scripts/cmd/getUserImportJobs.js"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.654.0",
    "@aws-sdk/client-sqs": "^3.654.0",
    "@aws-sdk/credential-providers": "^3.654.0",
    "@fastify/auth": "^4.6.1",
    "@fastify/awilix": "^5.1.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/schedule": "^4.1.1",
    "@fastify/swagger": "^8.15.0",
    "@lokalise/backend-http-client": "^2.3.0",
    "@lokalise/background-jobs-common": "^7.1.0",
    "@lokalise/fastify-extras": "^23.2.0",
    "@lokalise/healthcheck-utils": "^1.3.1",
    "@lokalise/id-utils": "^2.2.0",
    "@lokalise/node-core": "^12.3.2",
    "@lokalise/zod-extras": "^2.1.0",
    "@message-queue-toolkit/amqp": "^16.1.1",
    "@message-queue-toolkit/core": "^17.1.0",
    "@message-queue-toolkit/schemas": "^4.0.0",
    "@message-queue-toolkit/sns": "^17.2.0",
    "@message-queue-toolkit/sqs": "^17.1.1",
    "@prisma/client": "^5.20.0",
    "@prisma/instrumentation": "^5.20.0",
    "@scalar/fastify-api-reference": "^1.25.17",
    "@supercharge/promise-pool": "^3.2.0",
    "amqplib": "^0.10.4",
    "awilix": "^11.0.0",
    "awilix-manager": "^5.5.0",
    "bullmq": "^5.13.2",
    "fastify": "^4.28.1",
    "fastify-custom-healthcheck": "^3.1.0",
    "fastify-graceful-shutdown": "^4.0.1",
    "fastify-metrics": "^11.0.0",
    "fastify-no-icon": "^6.0.0",
    "fastify-type-provider-zod": "^2.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "layered-loader": "^13.2.0",
    "newrelic": "12.5.1",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "prom-client": "^15.1.3",
    "redis-semaphore": "^5.6.0",
    "toad-scheduler": "^3.0.1",
    "undici": "^6.19.8",
    "validation-utils": "^11.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@amplitude/analytics-types": "^2.8.2",
    "@biomejs/biome": "1.9.2",
    "@types/amqplib": "^0.10.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/newrelic": "^9.14.4",
    "@types/node": "^22.7.4",
    "@vitest/coverage-v8": "^2.1.1",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "mockttp": "^3.15.2",
    "oas-validator": "^5.0.8",
    "prisma": "^5.20.0",
    "prisma-lint": "^0.6.0",
    "ts-deepmerge": "^7.0.1",
    "tsx": "^4.19.1",
    "type-fest": "^4.26.1",
    "typescript": "5.6.2",
    "vitest": "^2.0.5",
    "yaml": "^2.5.1"
  }
}
